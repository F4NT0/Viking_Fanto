main	
	ldi	r4,string
	ldi	r3,cont
	ldi	r6,size
loop
	add	r4,r6
	sub	r6,1
	ldb	r1,r4
	add	r4,r3
	add	r3,1
	ldb	r2,r4
	sub	r5,r6,r3
	bnz	r5,testeLetras
	bez	r5,fim

testeLetras
	sub	r5,r1,r2
	bnz	r5,fail
	bez	r5,loop
fail
	ldi	r5,0
	stw	r5,0xf002
	hcf
fim
	ldi	r5,1
	stw	r5,0xf002
	hcf

string	"ana"
size	2
cont	0
	
	

